{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'TransactionForm',\n  props: {\n    accountId: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      transaction: {\n        amount: 0,\n        type: 'credit'\n      }\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        await axios.post(`http://localhost:3000/transactions/${this.accountId}`, this.transaction);\n        alert('Transaction ajoutée avec succès!');\n        this.transaction.amount = 0;\n        this.transaction.type = 'credit';\n        this.$emit('transaction-created'); // Émettre un événement personnalisé\n      } catch (error) {\n        console.error('Erreur lors de lajout de la transaction', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","accountId","type","Number","required","data","transaction","amount","methods","submitForm","post","alert","$emit","error","console"],"sources":["/Users/djahnickefrei/monProjet/bank-frontend/src/components/TransactionForm.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Ajouter une Transaction</h2>\n    <form @submit.prevent=\"submitForm\">\n      <div>\n        <label for=\"amount\">Montant:</label>\n        <input v-model.number=\"transaction.amount\" type=\"number\" id=\"amount\" required>\n      </div>\n      <div>\n        <label for=\"type\">Type:</label>\n        <select v-model=\"transaction.type\" id=\"type\">\n          <option value=\"credit\">Crédit</option>\n          <option value=\"debit\">Débit</option>\n        </select>\n      </div>\n      <button type=\"submit\">Ajouter Transaction</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'TransactionForm',\n  props: {\n    accountId: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      transaction: {\n        amount: 0,\n        type: 'credit'\n      }\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        await axios.post(`http://localhost:3000/transactions/${this.accountId}`, this.transaction);\n        alert('Transaction ajoutée avec succès!');\n        this.transaction.amount = 0;\n        this.transaction.type = 'credit';\n        this.$emit('transaction-created'); // Émettre un événement personnalisé\n      } catch (error) {\n        console.error('Erreur lors de lajout de la transaction', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* ... */\n</style>\n"],"mappings":"AAqBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;QACXC,MAAM,EAAE,CAAC;QACTL,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDM,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMX,KAAK,CAACY,IAAI,CAAE,sCAAqC,IAAI,CAACT,SAAU,EAAC,EAAE,IAAI,CAACK,WAAW,CAAC;QAC1FK,KAAK,CAAC,kCAAkC,CAAC;QACzC,IAAI,CAACL,WAAW,CAACC,MAAK,GAAI,CAAC;QAC3B,IAAI,CAACD,WAAW,CAACJ,IAAG,GAAI,QAAQ;QAChC,IAAI,CAACU,KAAK,CAAC,qBAAqB,CAAC,EAAE;MACrC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yCAAyC,EAAEA,KAAK,CAAC;MACjE;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}